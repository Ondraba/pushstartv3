{
    "name": "pushstart",
    "version": "0.1.0-alpha.1",
    "description": "Pushstart v3",
    "author": "pushstart",
    "license": "ISC",
    "homepage": "pushstart",
    "repository": {
        "type": "git",
        "url": "git@github.com:ApiTreeCZ/apitree-public.git"
    },
    "scripts": {
        "generate:lang": "ts-node --compilerOptions {\\\"module\\\":\\\"commonjs\\\"} ./scripts/default-lang.ts",
        "generate:graphql:merge": "node -e \"console.log(require('graphql-import').importSchema('./schema/root.graphql'))\" > graphql.schema.graphql",
        "generate:graphql:json": "npm run generate:graphql:merge && apollo-codegen introspect-schema graphql.schema.graphql --output graphql.schema.json",
        "generate:graphql:typescript": "gql-gen --schema graphql.schema.json --template graphql-codegen-typescript-template --config ./gqlgen.json --out ./shared/graphql.ts",
        "generate:graphql": "npm run generate:graphql:json && npm run generate:graphql:typescript",
        "prebuild": "rimraf -- dist .next",
        "build": "npm run generate:graphql && next build && tsc --project tsconfig.server.json && npm run generate:lang",
        "start": "NODE_ENV=production node dist/server/index.js",
        "dev": "npm run generate:graphql && npm run generate:lang && nodemon server/index.ts",
        "dev:server": "nodemon server/index-dev.ts",
        "dev:client": "npm run generate:graphql && npm run generate:lang && tsc --project tsconfig.server.json && node dist/server/index.js",
        "debug": "node --inspect dist/index.js",
        "tslint": "tslint -c tslint.json 'client/**/*.*' 'pages/**/*.*' 'server/**/*.*'",
        "tslint:check": "tslint-config-prettier-check ./tslint.json",
        "prettier:show": "prettier --list-different \"./**/*\"",
        "prettier:write": "prettier \"./**/*\" --write",
        "precommit": "pretty-quick --staged && npm run tslint",
        "test": "jest",
        "coverage": "jest --coverage",
        "coverage:report": "npm run coverage && cat ./coverage/lcov.info | coveralls"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "collectCoverageFrom": [
            "client/**/*.ts"
        ]
    },
    "dependencies": {
        "@material-ui/core": "3.0.1",
        "@material-ui/icons": "3.0.1",
        "@types/ramda": "^0.25.38",
        "accepts": "1.3.5",
        "apollo-boost": "0.1.15",
        "apollo-link-context": "1.0.8",
        "apollo-server-express": "2.0.4",
        "autoprefixer": "9.1.3",
        "bcrypt": "3.0.0",
        "body-parser": "1.18.3",
        "bowser": "2.0.0-beta.1",
        "classnames": "2.2.6",
        "clean-css": "4.2.1",
        "draft-js": "0.10.5",
        "express": "4.16.3",
        "formik": "^1.2.0",
        "glob": "7.1.3",
        "graphql": "0.13.2",
        "graphql-import": "0.6.0",
        "graphql-schema-tools": "0.0.6",
        "graphql-tag": "2.9.2",
        "graphql-tools": "3.1.1",
        "immutable": "3.8.2",
        "intl": "1.2.5",
        "isomorphic-unfetch": "2.1.1",
        "js-cookie": "2.2.0",
        "jsonwebtoken": "8.3.0",
        "mongoose": "5.2.10",
        "next": "6.1.1",
        "nprogress": "0.2.0",
        "postcss": "7.0.2",
        "ramda": "^0.25.0",
        "react": "16.4.2",
        "react-apollo": "2.1.11",
        "react-dom": "16.4.2",
        "react-ga": "2.5.3",
        "react-google-maps": "9.4.5",
        "react-intl": "2.4.0",
        "react-markdown": "3.4.1",
        "react-syntax-highlighter": "8.0.1",
        "reselect": "3.0.1",
        "typescript-logging": "^0.6.3"
    },
    "devDependencies": {
        "@playlyfe/gql": "2.6.0",
        "@types/accepts": "1.3.5",
        "@types/autoprefixer": "6.7.3",
        "@types/bcrypt": "2.0.0",
        "@types/body-parser": "1.17.0",
        "@types/classnames": "2.2.6",
        "@types/cookie": "0.3.1",
        "@types/draft-js": "0.10.24",
        "@types/express": "4.16.0",
        "@types/glob": "5.0.35",
        "@types/intl": "1.2.0",
        "@types/isomorphic-fetch": "0.0.34",
        "@types/jest": "23.3.1",
        "@types/js-cookie": "2.1.0",
        "@types/jsonwebtoken": "7.2.8",
        "@types/jss": "9.5.5",
        "@types/mongoose": "5.2.9",
        "@types/next": "6.1.4",
        "@types/node": "10.9.3",
        "@types/nprogress": "0.0.29",
        "@types/react": "16.4.13",
        "@types/react-intl": "2.3.10",
        "@types/styled-jsx": "2.2.5",
        "@zeit/next-typescript": "1.1.0",
        "babel-plugin-module-resolver": "3.1.1",
        "babel-plugin-react-intl": "2.4.0",
        "dotenv": "6.0.0",
        "fork-ts-checker-webpack-plugin": "0.4.9",
        "graphql-code-generator": "0.11.0",
        "graphql-codegen-typescript-template": "0.11.0",
        "husky": "0.14.3",
        "jest": "23.5.0",
        "prettier": "1.14.2",
        "pretty-quick": "1.6.0",
        "rimraf": "2.6.2",
        "ts-jest": "23.1.4",
        "ts-node": "7.0.1",
        "tslint": "5.11.0",
        "tslint-config-prettier": "1.15.0",
        "tslint-react": "3.6.0",
        "typescript": "3.0.3"
    }
}
